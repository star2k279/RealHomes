        </div>
        <div class="services-grid">
            <div class="col-md-6 service-top1">
                <div class=" ser-grid">
                    <a href="#" class="hi-icon hi-icon-archive glyphicon glyphicon-user"> </a>
                </div>
                <div class="ser-top">
                    <h4>@Umbraco.Field("serviceTitle1")</h4>
                    <p>
                        @Umbraco.Field("serviceDescription1")
