@inherits Umbraco.Web.Mvc.UmbracoViewPage


<!--service-->
<div class="services">
    <div class="container">
        <div class="service-top">
            @Umbraco.Field("serviceMainTitle", insertBefore: "<h3>", insertAfter: "</h3>", casing: RenderFieldCaseType.Upper)
